<assembly>
    <id>diameter-mux-bean</id>
    <formats>
        <format>dir</format>
    </formats>
    <baseDirectory>main</baseDirectory>

    <fileSets>
        <fileSet>
            <directory>src/main/module</directory>
            <outputDirectory>/</outputDirectory>
            <filtered>true</filtered>
        </fileSet>
    </fileSets>

    <dependencySets>
        <dependencySet>
            <outputDirectory>/</outputDirectory>
            <useTransitiveDependencies>true</useTransitiveDependencies>
        </dependencySet>
    </dependencySets>
</assembly>
